    },
    "license": "MIT",
    "maintainer": [
        {
            "name": "alexAubin",
            "email": "alex.aubin@mailoo.org",
            "url": "https://github.com/alexAubin/"
        },
        {
            "name": "ljf"
        },
        {
            "name": "yalh76"
        }
    ],
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/wekan",
                "default": "/wekan"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": false
            },
            {
                "name": "admin",
                "type": "user"
            }
        ]
    }
}
