{
    "name": "Wekan",
    "id": "wekan",
    "packaging_format": 1,
    "description": {
        "en": "Trello-like kanban",
        "fr": "Un kanban similaire Ã  Trello"
    },
    "version": "6.28~ynh1",
    "url": "https://wekan.github.io",
    "upstream": {
        "license": "MIT",
        "website": "https://wekan.github.io",
